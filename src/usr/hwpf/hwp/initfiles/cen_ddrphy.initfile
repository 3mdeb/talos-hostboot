# scom 0x8000(00,04,08,0C,10)770301143f {	# CONFIG1_P0_[0:4]
scom 0x800(0,1)3C770301143F {	# CONFIG1_P[0:1] broadcast [0:4]
	bits	,	scom_data	,	expr							;
