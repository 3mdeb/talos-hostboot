	59		,	0b0			,	any								;	# CE0DLTVCCD1, must be 0
	60		,	0b0			,	any								;	# CE0DLTVCCD2, must be 0
	61		,	0b0			,	any								;	# S0INSDLYTAP, must be 0
	62		,	0b0			,	any								;	# S1INSDLYTAP, must be 0
