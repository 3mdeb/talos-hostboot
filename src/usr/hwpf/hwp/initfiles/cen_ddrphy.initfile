	48:63	,	0x8580		,	((def_is_mba01) && (def_is_x4))						;	# x4 swizzle quad2/3
	48:63	,	0xC0C0		,	((def_is_mba01) && (def_is_x8) && (def_is_type1))	;	# x8 swizzle quad 2/3
