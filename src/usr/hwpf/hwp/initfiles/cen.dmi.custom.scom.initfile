}
# ./iotk put rx_wt_timeout_sel=111
# ./iotk put rx_ds_bl_timeout_sel=101
# ./iotk put rx_ds_timeout_sel=110
#./iotk put rx_sls_timeout_sel=111
# 0x800898000201043F
scom 0x800.0b(rx_timeout_sel_pg)(rx_grp0)(lane_na).0x(cn_gcr_addr) {
bits,                     scom_data;
