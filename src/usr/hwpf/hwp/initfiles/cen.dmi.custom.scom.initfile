# ./iotk put rx_servo_timeout_sel_D=1001
# 0x800B60000201043F
scom 0x800.0b(rx_servo_to1_pp)(rx_grp0)(lane_na).0x(cn_gcr_addr) {
bits,                     scom_data;
rx_servo_timeout_sel_d,   0b1001;
}
# ./iotk put rx_servo_timeout_sel_H=1110
# 0x800B68000201043F
