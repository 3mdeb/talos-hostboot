bits,                     scom_data;
rx_servo_timeout_sel_h,   0b1110;
}
# ./iotk put rx_servo_timeout_sel_I=1011
# ./iotk put rx_servo_timeout_sel_J=1100
# 0x800B70000201043F
scom 0x800.0b(rx_servo_to3_pp)(rx_grp0)(lane_na).0x(cn_gcr_addr) {
bits,                     scom_data;
