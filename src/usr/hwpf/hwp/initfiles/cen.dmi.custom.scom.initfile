rx_eo_ctle_timeout_sel,   0b111;
rx_eo_h1ap_timeout_sel,   0b111;
rx_eo_ddc_timeout_sel,    0b111;
}


scom 0x800.0b(rx_eo_convergence_pg)(rx_grp0)(lane_na).0x(cn_gcr_addr) {
bits,                               scom_data;
rx_eo_converged_end_count,          0b111;
}
