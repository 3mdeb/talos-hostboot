# PSI Host Bridge FIR Action0 Register
scom 0x02010906 {
    bits,    scom_data;
    0:63,    0x0000000000000000;
}

# PSI Host Bridge FIR Action1 Register
scom 0x02010907 {
    bits,    scom_data;
    0:63,    0xC62B000000000000;
}

# PSI Host Bridge FIR Mask Register
scom 0x02010903 {
    bits,    scom_data;
    0:63,    0x3900FFF800000000;
}
