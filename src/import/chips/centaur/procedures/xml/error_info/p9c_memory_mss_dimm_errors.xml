    <ffdc>UPDATE_BAD_DQ_BITMAP_RANK0</ffdc>
    <ffdc>UPDATE_BAD_DQ_BITMAP_RANK1</ffdc>
    <ffdc>UPDATE_BAD_DQ_BITMAP_RANK2</ffdc>
    <ffdc>UPDATE_BAD_DQ_BITMAP_RANK3</ffdc>
    <callout>
      <target>DIMM</target>
      <priority>HIGH</priority>
    </callout>
  </hwpError>
